	"homepage": "https://flur34.com",
	"bugs": "https://github.com/flur34/flur34/issues",
	"repository": "https://github.com/flur34/flur34",
	"readme": "README.md",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"lint": "prettier . --check . && eslint .",
		"format": "prettier . --write .",
		"coverage": "vitest run --coverage",
		"test:unit": "vitest run",
		"test:integration": "playwright test",
		"verify": "concurrently --names \"unit,integration,svelte\" \"pnpm -S test:unit\" \"pnpm -S test:integration\" \"pnpm -S check\"",
		"prepare": "husky || true"
	},
	"devDependencies": {
		"@eslint/css": "^0.11.0",
		"@eslint/eslintrc": "^3.3.1",
		"@eslint/js": "^9.35.0",
		"@eslint/json": "^0.13.2",
		"@playwright/test": "^1.55.0",
		"@sveltejs/adapter-auto": "^6.1.0",
		"@sveltejs/adapter-node": "^5.3.2",
		"@sveltejs/adapter-static": "^3.0.9",
		"@sveltejs/kit": "^2.41.0",
		"@sveltejs/vite-plugin-svelte": "^6.2.0",
		"@testing-library/svelte": "^5.2.8",
		"@typescript-eslint/eslint-plugin": "^8.44.0",
		"@typescript-eslint/parser": "^8.44.0",
		"@vitest/coverage-v8": "3.2.4",
		"concurrently": "^9.2.1",
		"eslint": "^9.35.0",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-svelte": "^3.12.3",
		"fake-indexeddb": "^6.2.2",
		"globals": "^16.4.0",
		"husky": "^9.1.7",
		"jiti": "^2.5.1",
		"jsdom": "^26.1.0",
		"prettier": "^3.6.2",
		"prettier-plugin-svelte": "^3.4.0",
		"sass": "^1.92.1",
		"svelte": "^5.38.10",
		"svelte-check": "^4.3.1",
		"svelte-eslint-parser": "^1.3.2",
		"svelte-preprocess": "^6.0.3",
		"tslib": "^2.8.1",
		"typescript": "^5.9.2",
		"typescript-eslint": "^8.44.0",
		"vite": "^7.1.5",
		"vite-plugin-pwa": "^1.0.3",
		"vitest": "^3.2.4"
	},
	"type": "module",
	"dependencies": {
		"firebase": "^12.2.1",
		"smol-toml": "^1.4.2"
	},
	"packageManager": "pnpm@10.16.0"
}
